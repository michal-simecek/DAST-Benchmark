{
  "project_options": {
    "misc": {
      "collaborator_server": {
        "location": "burp.tvmglobal.com",
        "poll_over_unencrypted_http": false,
        "polling_location": "",
        "type": "private"
      }
    },
    "embedded_browser": {
      "allow_running_without_sandbox": true
    },
    "http_message_search": {
      "autoscroll_to_match_by_default": true,
      "case_sensitive_by_default": false,
      "regex_by_default": false
    }
  },
  "user_options": {
    "display": {
      "html_rendering": {
        "allow_http_requests": true,
        "enable_embedded_browser_sandbox": true,
        "allow_running_without_browser_sandbox": true
      }
    },
    "connections": {
      "upstream_proxy": {
        "servers": [
          {
            "destination_host": "*portswigger.net",
            "enabled": false
          },
          {
            "destination_host": "*portswigger-cdn.net",
            "enabled": false
          },
          {
            "destination_host": "*oastify.com",
            "enabled": false
          },
          {
            "destination_host": "*burp.tvmglobal.com",
            "enabled": false
          },
          {
            "destination_host": "*raw.githubusercontent.com",
            "enabled": false
          }
        ]
      }
    },
    "misc": {
      "hotkeys": [
        {
          "action": "send_to_repeater",
          "hotkey": "Ctrl+R"
        },
        {
          "action": "send_to_intruder",
          "hotkey": "Ctrl+I"
        },
        {
          "action": "forward_intercepted_proxy_message",
          "hotkey": "Ctrl+F"
        },
        {
          "action": "toggle_proxy_interception",
          "hotkey": "Ctrl+T"
        },
        {
          "action": "issue_repeater_request",
          "hotkey": "Ctrl+Space"
        },
        {
          "action": "switch_to_target",
          "hotkey": "Ctrl+Shift+T"
        },
        {
          "action": "switch_to_proxy",
          "hotkey": "Ctrl+Shift+P"
        },
        {
          "action": "switch_to_intruder",
          "hotkey": "Ctrl+Shift+I"
        },
        {
          "action": "switch_to_repeater",
          "hotkey": "Ctrl+Shift+R"
        },
        {
          "action": "switch_to_project_options",
          "hotkey": "Ctrl+Shift+O"
        },
        {
          "action": "go_to_previous_tab",
          "hotkey": "Ctrl+Minus"
        },
        {
          "action": "go_to_next_tab",
          "hotkey": "Ctrl+Equals"
        },
        {
          "action": "editor_cut",
          "hotkey": "Ctrl+X"
        },
        {
          "action": "editor_copy",
          "hotkey": "Ctrl+C"
        },
        {
          "action": "editor_paste",
          "hotkey": "Ctrl+V"
        },
        {
          "action": "editor_undo",
          "hotkey": "Ctrl+Z"
        },
        {
          "action": "editor_redo",
          "hotkey": "Ctrl+Y"
        },
        {
          "action": "editor_select_all",
          "hotkey": "Ctrl+A"
        },
        {
          "action": "editor_search",
          "hotkey": "Ctrl+S"
        },
        {
          "action": "editor_go_to_previous_search_match",
          "hotkey": "Ctrl+Comma"
        },
        {
          "action": "editor_go_to_next_search_match",
          "hotkey": "Ctrl+Period"
        },
        {
          "action": "editor_url_decode",
          "hotkey": "Ctrl+Shift+U"
        },
        {
          "action": "editor_url_encode_key_characters",
          "hotkey": "Ctrl+U"
        },
        {
          "action": "editor_url_encode_all_characters",
          "hotkey": "Ctrl+Alt+U"
